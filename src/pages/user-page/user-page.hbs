<main>
    <form class="change-all-page">
        <div class="change_all-page__avatar-container">
            <img src="../assets/Птица-секретарь.png" alt="Профиль" class="chat-page-profile-icon"/>
        </div>
        <div class="change_all-page__nickname">
            <h2 class="chat-page__username">Динка</h2>
        </div>
        <div class="change_all-page__content">
            {{> InputField className="change_all-page__input" title="Почта" name="email" value="example@example.com" }}
            {{> InputField className="change_all-page__input" title="Логин" name="login" value="example_login" }}
            {{> InputField className="change_all-page__input" title="Имя" name="first_name" value="Дина" }}
            {{> InputField className="change_all-page__input" title="Фамилия" name="second_name" value="Пупина" }}
            {{> InputField className="change_all-page__input" title="Имя в чате" name="display_name" value="Динка" }}
            {{> InputField className="change_all-page__input" title="Телефон" name="phone" value="8-925-111-11-11" }}
        </div>
        <nav class="change_all-page__footer">
            {{> Link page="changeData" text="Изменить данные"}}
            {{> Link page="changeData" text="Изменить пароль"}}
            {{> Link page="start" text="Выйти"}}
        </nav>
    </form>
</main>
